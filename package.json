{
  "name": "tasknotes",
  "sideEffects": false,
  "version": "0.0.1",
  "description": "Task notes app.",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --exclude 'src/tests/*.{ts,tsx}'",
    "start": "webpack-dev-server",
    "test": "jest",
    "build": "tsc && webpack --progress",
    "ts": "tsc",
    "clean": "rd /s/q built",
    "prepare": "tsc && webpack --progress --config webpack.prod.config.js"
  },
  "author": "DM",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "mini.css": "^3.0.1",
    "preact": "^8.5.2",
    "preact-compat": "^3.19.0",
    "primeflex": "^1.0.0",
    "primeicons": "^1.0.0",
    "primereact": "^3.3.2",
    "react-redux": "^6.0.1",
    "react-router": "4.3.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-crud": "^3.3.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/history": "^5.0.0",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.17.0",
    "@types/react": "^16.9.10",
    "@types/react-dom": "^16.9.2",
    "@types/react-redux": "^6.0.14",
    "@types/react-router": "^4.4.5",
    "@types/react-router-config": "^1.1.3",
    "@types/react-router-dom": "4.3.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "less": "^3.10.3",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "react-test-renderer": "^16.11.0",
    "redux-devtools-extension": "^2.13.8",
    "source-map-loader": "^0.2.4",
    "ts-import-plugin": "^1.6.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.4.5",
    "tslint": "^5.20.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.6.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
